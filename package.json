{
  "name": "jsreport-scripts",
  "version": "2.4.0",
  "description": "jsreport extension capable of running custom javascript functions during the rendering process",
  "scripts": {
    "start": "jsreport-studio-start --httpPort 4300",
    "test": "mocha test --timeout 7000 && standard",
    "build": "jsreport-studio-build",
    "prepublish": "in-publish && jsreport-studio-build || not-in-publish"
  },
  "homepage": "https://github.com/jsreport/jsreport-scripts",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport-scripts.git"
  },
  "keywords": [
    "jsreport"
  ],
  "standard": {
    "ignore": [
      "main.js"
    ],
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    }
  },
  "license": "LGPL",
  "dependencies": {
    "bluebird": "3.5.4",
    "node.extend.without.arrays": "1.1.6"
  },
  "author": "Jan Blaha",
  "devDependencies": {
    "babel-eslint": "8.2.3",
    "eslint": "4.19.1",
    "eslint-config-standard-jsx": "5.0.0",
    "eslint-plugin-babel": "5.2.0",
    "in-publish": "2.0.1",
    "jsreport-assets": "1.6.0",
    "jsreport-core": "2.9.0",
    "jsreport-jsrender": "2.1.1",
    "jsreport-studio-dev": "1.5.0",
    "jsreport-templates": "2.4.0",
    "mocha": "5.2.0",
    "should": "13.2.3",
    "standard": "11.0.1"
  },
  "engines": {
    "node": ">=8.9"
  },
  "files": [
    "lib",
    "studio",
    "index.js",
    "jsreport.config.js"
  ]
}
