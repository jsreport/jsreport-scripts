{
  "name": "jsreport-scripts",
  "version": "1.3.0",
  "description": "jsreport extension capable of running custom javascript functions during the rendering process",
  "scripts": {
    "test": "mocha test --timeout 5000 && standard",
    "build": "jsreport-studio-build",
    "prepublish": "in-publish && jsreport-studio-build || not-in-publish"
  },
  "homepage": "https://github.com/jsreport/jsreport-scripts",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport-scripts.git"
  },
  "keywords": [
    "jsreport"
  ],
  "standard": {
    "ignore": [
      "main.js"
    ],
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    }
  },
  "license": "LGPL",
  "dependencies": {
    "bluebird": "3.4.7",
    "in-publish": "2.0.0",
    "shortid": "2.2.6"
  },
  "author": "Jan Blaha",
  "devDependencies": {
    "babel-eslint": "7.1.1",
    "eslint": "3.15.0",
    "eslint-config-standard-jsx": "3.3.0",
    "eslint-plugin-babel": "4.0.1",
    "jsreport-core": "1.1.1",
    "jsreport-jsrender": "1.0.1",
    "jsreport-studio-dev": "1.0.2",
    "jsreport-templates": "1.1.0",
    "mocha": "3.2.0",
    "requirejs": "2.3.2",
    "should": "11.2.0",
    "standard": "8.6.0",
    "string": "3.3.3"
  },
  "files": [
    "lib",
    "studio",
    "index.js",
    "jsreport.config.js"
  ]
}
